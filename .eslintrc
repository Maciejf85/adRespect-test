{
  "env": { "browser": true },
  "extends": ["airbnb", "prettier"],
  "rules": {
    "import/no-extraneous-dependencies": [
      "off",
      { "devDependencies": false, "optionalDependencies": false, "peerDependencies": false }
    ],
    "no-console": "off",
    "import/prefer-default-export": ["off", { "target": "any" }]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
